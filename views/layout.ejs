<!DOCTYPE html>
<html ng-app="leansite">

  <head>
    <title>LEAN site</title>
    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="theme-color" content="#ffffff">

    <!--STYLES-->
    <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap-theme.css">
    <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="/node_modules/angular-material/angular-material.min.css">
    <link rel="stylesheet" href="/node_modules/summernote/dist/summernote.css">
    <link rel="stylesheet" href="/css/materialize.css">
    <link rel="stylesheet" href="/css/master.css">
    <!--STYLES END-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <base href="/">

    <!--Google Analytics-->
    <script>
      (function (i, s, o, g, r, a, m) {
      i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
        (i[r].q = i[r].q || []).push(arguments)
      }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
      })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
      ga('create', 'UA-90530953-3', 'auto');
      ga('send', 'pageview');
    </script>

  </head>

  <body ng-controller="MainController as mc" ng-cloak layout="column" layout-gt-sm="row" layout-wrap>
    <tool-bar flex-gt-sm="100"></tool-bar>

    <message-container layout="row" layout-align="center center" ng-if="mc.error || mc.message">
      <div layout="column">
        <div layout-padding ng-if="mc.error">
          <p>Error: <span class="red-text text-darken-1" ng-bind="mc.error"></span></p>
        </div>
        <div layout-padding ng-if="mc.message">
          <pre class="text-info md-padding" ng-bind="mc.message"></pre>
        </div>
      </div>
      <md-button class="md-fab md-mini md-primary" md-no-ink ng-click="mc.error = false; mc.message = false">&times;</md-button>
    </message-container>

    <side-nav layout="column"></side-nav>

    <div ng-view flex-gt-sm>
      <!-- ng-view container -->
    </div>

    <footer flex-gt-sm="100" layout-padding class="blue-grey darken-2">
      <ul flex-gt-sm class="container">
        <li><a ng-href="/about" class="white-text">About</a></li>

        <li><a ng-href="/contact" class="white-text">Contact Us</a></li>

        <li><a class="white-text" href="mailto:shingo.it@usu.edu">Technical Support</a></li>

        <li><a ng-href="/feedback" class="white-text">Send us your feedback</a></li>
      </ul>
    </footer>



    <!--SCRIPTS-->
    <script src="/node_modules/jquery/dist/jquery.min.js"></script>
    <script src="/node_modules/lodash/lodash.min.js"></script>
    <script src="/node_modules/summernote/dist/summernote.min.js"></script>
    <script src="/node_modules/core-js/client/shim.min.js"></script>
    <script src="/node_modules/zone.js/dist/zone.min.js"></script>
    <script src="/node_modules/reflect-metadata/Reflect.js"></script>
    <script src="/node_modules/systemjs/dist/system.src.js"></script>
    <script src="/node_modules/trix/dist/trix.js"></script>
    <script src="/node_modules/moment/moment.js"></script>
    <script src="/node_modules/angular/angular.js"></script>
    <script src="/node_modules/angular-route/angular-route.js"></script>
    <script src="/node_modules/angular-aria/angular-aria.js"></script>
    <script src="/node_modules/angular-animate/angular-animate.js"></script>
    <script src="/js/angular.min.js"></script>
    <script src="/node_modules/angular-messages/angular-messages.js"></script>
    <script src="/node_modules/angular-sanitize/angular-sanitize.js"></script>
    <script src="/node_modules/angular-cookies/angular-cookies.js"></script>
    <script src="/node_modules/angular-moment/angular-moment.js"></script>
    <script src="/node_modules/angular-trix/dist/angular-trix.js"></script>
    <script src="/node_modules/angular-bootstrap/ui-bootstrap.js"></script>
    <script src="/node_modules/angular-summernote/dist/angular-summernote.js"></script>
    <script src="/src/app/app.js"></script>
    <script src="/src/app/controllers/AddCommentController.js"></script>
    <script src="/src/app/controllers/AddEntryController.js"></script>
    <script src="/src/app/controllers/AdminController.js"></script>
    <script src="/src/app/controllers/AnswerController.js"></script>
    <script src="/src/app/controllers/AuthController.js"></script>
    <script src="/src/app/controllers/CommentController.js"></script>
    <script src="/src/app/controllers/DashboardController.js"></script>
    <script src="/src/app/controllers/EmailController.js"></script>
    <script src="/src/app/controllers/EntryDetailController.js"></script>
    <script src="/src/app/controllers/EntryHomeController.js"></script>
    <script src="/src/app/controllers/FlagContentController.js"></script>
    <script src="/src/app/controllers/MainController.js"></script>
    <script src="/src/app/controllers/ModeratorController.js"></script>
    <script src="/src/app/controllers/NavController.js"></script>
    <script src="/src/app/controllers/ProfileController.js"></script>
    <script src="/src/app/controllers/QuestionController.js"></script>
    <script src="/src/app/controllers/ResetController.js"></script>
    <script src="/src/app/controllers/SettingsController.js"></script>
    <script src="/src/app/directives/AdminDirective.js"></script>
    <script src="/src/app/directives/AnswerDirective.js"></script>
    <script src="/src/app/directives/CommentDirective.js"></script>
    <script src="/src/app/directives/EntryRecentDirective.js"></script>
    <script src="/src/app/directives/EntrySummaryDirective.js"></script>
    <script src="/src/app/directives/FlagDirective.js"></script>
    <script src="/src/app/directives/IsSameDirective.js"></script>
    <script src="/src/app/directives/ModeratorDirective.js"></script>
