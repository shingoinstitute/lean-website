<!DOCTYPE html>
<html ng-app="leansite" ng-cloak>

<head>
  <title>LEAN site</title>
  <!-- Viewport mobile tag for sensible mobile support -->
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
  <link rel="manifest" href="/manifest.json">
  <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="theme-color" content="#ffffff">

  <!--STYLES-->
  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap-theme.css">
  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap-theme.min.css">
  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.css">
  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="/bower_components/angular-material/angular-material.css">
  <link rel="stylesheet" href="/bower_components/summernote/dist/summernote.css">
  <link rel="stylesheet" href="/css/materialize.css">
  <link rel="stylesheet" href="/css/master.css">
  <!--STYLES END-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <base href="/">



  <!--Google Analytics-->
  <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-90530953-3', 'auto');
  ga('send', 'pageview');
  </script>

</head>

<body id="main" ng-controller="MainController as mc" ng-cloak layout="column" layout-gt-sm="row" layout-wrap>
  <tool-bar flex="100"></tool-bar>
  <message-container layout="row" layout-align="center center" ng-if="mc.error || mc.message">
    <div layout="column">
      <div layout-padding ng-if="mc.error">
        <p>Error: <span class="red-text text-darken-1" ng-bind="mc.error"></span></p>
      </div>
      <div layout-padding ng-if="mc.message">
        <pre class="text-info md-padding" ng-bind="mc.message"></pre>
      </div>
    </div>
    <md-button class="md-fab md-mini md-primary" md-no-ink ng-click="mc.error = false; mc.message = false">&times;</md-button>
  </message-container>
  <side-nav layout="column"></side-nav>
  <div ng-view flex><!-- ng-view container --></div>
  <footer flex="100" layout-padding class="blue-grey darken-2">
    <ul class="container">
      <li><a ng-href="/about" class="white-text">About</a></li>
      <li><a ng-href="/contact" class="white-text">Contact Us</a></li>
      <li><a ng-href="/feedback" class="white-text">Send us your feedback</a></li>
    </ul>
  </footer>

  <!--SCRIPTS-->
  <script src="/js/dependencies/sails.io.js"></script>
  <script src="/bower_components/jquery/dist/jquery.js"></script>
  <script src="/bower_components/bootstrap/dist/js/bootstrap.js"></script>
  <script src="/bower_components/lodash/lodash.js"></script>
  <script src="/bower_components/summernote/dist/summernote.js"></script>
  <script src="/bower_components/angular/angular.js"></script>
  <script src="/bower_components/angular-route/angular-route.js"></script>
  <script src="/bower_components/angular-aria/angular-aria.js"></script>
  <script src="/bower_components/angular-animate/angular-animate.js"></script>
  <script src="/bower_components/angular-material/angular-material.js"></script>
  <script src="/bower_components/angular-messages/angular-messages.js"></script>
  <script src="/bower_components/angular-sanitize/angular-sanitize.js"></script>
  <script src="/bower_components/angular-cookies/angular-cookies.min.js"></script>
  <script src="/bower_components/trix/dist/trix.js"></script>
  <script src="/bower_components/angular-trix/dist/angular-trix.js"></script>
  <script src="/bower_components/moment/moment.js"></script>
  <script src="/bower_components/angular-moment/angular-moment.js"></script>
  <script src="/bower_components/angular-summernote/dist/angular-summernote.js"></script>
  <script src="/js/app.js"></script>
  <script src="/js/controllers/AddCommentController.js"></script>
  <script src="/js/controllers/AddEntryController.js"></script>
  <script src="/js/controllers/AdminController.js"></script>
  <script src="/js/controllers/AnswerController.js"></script>
  <script src="/js/controllers/AuthController.js"></script>
  <script src="/js/controllers/CommentController.js"></script>
  <script src="/js/controllers/DashboardController.js"></script>
  <script src="/js/controllers/EntryDetailController.js"></script>
  <script src="/js/controllers/EntryHomeController.js"></script>
  <script src="/js/controllers/FlagContentController.js"></script>
  <script src="/js/controllers/MainController.js"></script>
  <script src="/js/controllers/ModeratorController.js"></script>
  <script src="/js/controllers/NavController.js"></script>
  <script src="/js/controllers/ProfileController.js"></script>
  <script src="/js/controllers/QuestionController.js"></script>
  <script src="/js/controllers/ResetController.js"></script>
  <script src="/js/controllers/SettingsController.js"></script>
  <script src="/js/directives/AdminDirective.js"></script>
  <script src="/js/directives/AnswerDirective.js"></script>
  <script src="/js/directives/CommentDirective.js"></script>
  <script src="/js/directives/EntryRecentDirective.js"></script>
  <script src="/js/directives/EntrySummaryDirective.js"></script>
  <script src="/js/directives/FlagDirective.js"></script>
  <script src="/js/directives/IsSameDirective.js"></script>
  <script src="/js/directives/ModeratorDirective.js"></script>
  <script src="/js/directives/ProfileDirective.js"></script>
  <script src="/js/directives/QuestionDirective.js"></script>
  <script src="/js/directives/SidenavDirective.js"></script>
  <script src="/js/directives/ToolbarDirective.js"></script>
  <script src="/js/services/authService.js"></script>
  <script src="/js/services/entryService.js"></script>
  <script src="/js/services/flagService.js"></script>
  <script src="/js/services/userService.js"></script>
  <!--SCRIPTS END-->
</body>

</html>
